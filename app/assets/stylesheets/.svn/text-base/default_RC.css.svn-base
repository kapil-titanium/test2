/* ================ Slider =====*/

#scrollerota {
	width: 695px;
	height: 250px;
	overflow: hidden;
	position: relative;
	background:#ffffcd;
	}

	#scrollerota ul.text {
		list-style: none;
		width: 80px;
		position: absolute;
		top: 0;
		left: 550px;
		padding: 0;
		margin: 0;
		color: #fff;
		font-size: 14px;
		line-height: 20px;
		}
		
		#scrollerota ul.text li {
			overflow: hidden;
			}

	#scrollerota a.sliderbtn {
		padding: 5px;
		text-align: center;
		color: #fff;
		clear: both;
		display: block;
		width: 40px;
		height:40px;
		margin-top: 16px;
		text-decoration: none;
		font-size: 12px;
		line-height: 17px;
		background-position:center;
		background-repeat:no-repeat;
		
		
		}
		
	#scrollerota a:hover.sliderbtn {
		text-decoration: none;
		
		}
	
	
	
	#scrollerota ul.images {
		list-style: none;
		position: absolute;
		top: 0;
		left: 0;
		padding: 0;
		margin: 0;
		}
		
		#scrollerota ul.images li {
			float: left;
			}
						
	#scrollerota .controls {
		position: absolute;
		bottom: 15px;
		right: 300px;
		display:none;
		}
		
		#scrollerota .controls a {
			width: 22px;
			height: 22px;
			display: block;
			float: left;
			background: url(controls.png) no-repeat;
			}
	
		#scrollerota .controls .prev {
			background-position: 0 -22px;
			}
	
		#scrollerota .controls .next {
			background-position: -23px -22px;
			}
			
		#scrollerota .controls .play {
			background-position: -23px 0;
			display: none;
			}

