<div class="K_Dish_Display">
    <div class="K_D_Head">
        <h1 class="pull-left">
            <a href="<%= employee_path(@employee) %>" style="text-decoration: none; color: inherit" path = "/orders/get_all_orders">Employee Section</a>
        </h1>
        <!-- <table class="pull-right" cellpadding="0" cellspacing="0" border="0">
            <tr>
                <td><a href="#" class="Btn_Add_M_Cart">Add selected meals to cart</a></td>
            </tr>
        </table> -->

        <div class="clearfix"></div>
        <div class="K_D_Filter_Row" style="border-bottom: 1px #DDDDDD solid;">
            <a href="<%= orders_path %>" >Orders</a>
        </div>
        <div class="K_D_Filter_Row path_link">
            <a style="cursor: pointer;" id ="orders_link" path = "/orders/get_all_orders" >All</a>
            <a style="cursor: pointer;" id ="pending_user_settlement_link" path = "/orders/open_parents" >Pending User Settlement</a>
            <a style="cursor: pointer;" id ="pending_kitchen_settlement_link" path = "/orders/children" >Pending Kitchen Settlement</a>
            <a style="cursor: pointer;" id ="settled_user_payments_link" path = "/orders/closed_in_payments" >Settled User Payments</a>
            <a style="cursor: pointer;" id ="settled_kitchen_payments_link" path = "/orders/closed_out_payments" >Settled Kitchen Payments</a>
            <a style="cursor: pointer;" id ="cancel_link" path = "/orders/cancel" >Cancel</a>
        </div><!--K_D_Filter_Row-->
    </div><!--end K_D_Head -->
    <div class="inner">
        <div id="MyBasketWraper" class="orders_data">
            <%= render :partial => '/orders/orders_list', :locals => {:orders => @orders} %>
        </div>
        <div class="clearfix"></div>
    </div>
</div><!-- end K_Dish_Display -->
<!--End Profile Info-->