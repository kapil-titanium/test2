<div class="TabbedPanelsContent">
    <div style="height: 400px;">

        <table width="100%" border="0" cellspacing="0" cellpadding="20" class="display ">
            <tr>
                <td width="25%" class="bada">First Name</td>
                <td width="25%" class="disabletxt dottedline"><%= @user.first_name %></td>
                <td width="25%" class="bada">Middle Name</td>
                <td width="25%" class="disabletxt"> <%= @user.middle_name   %></td>
            </tr>
            <tr class="even">
                <td class="bada">Last Name</td>
                <td class="disabletxt dottedline"><%= @user.last_name   %></td>
                <td class="bada">DOB</td>
                <td class="disabletxt"><%= @user.dob   %></td>
            </tr>
            <tr>
                <td class="bada">Gender</td>
                <td class="disabletxt dottedline"><%= @user.gender   %></td>
                <td class="bada">Food Type</td>
                <td class="disabletxt"><%= is_veg_text(@user.is_veg) %></td>
            </tr>
            <tr class="even">
                <td class="bada">Email Id</td>
                <td class="disabletxt dottedline"><%= @user.primary_email_id   %></td>
                <td class="bada">Alternative Id</td>
                <td class="disabletxt"><%= @user.alternate_email_id   %></td>
            </tr>

            <tr >
                <td class="bada">Mobile Number</td>
                <td class="disabletxt dottedline"><%= @user.primary_mobile_number   %></td>
                <td class="bada">Alternative Number</td>
                <td class="disabletxt"><%= @user.alternate_contact_number   %></td>
            </tr>

        </table>
    </div>
</div>